function calculateGrade() {

let marks;

while (true) {

marks= parseFloat(prompt("Enter student marks (0-100):"));

if (isNaN(marks) || marks < 0 && marks > 100 ) {

alert("The marks should be a number between 0 to 100. Please try again." );

} else {

break;

}

}

let grade;

if (marks > 79) {

grade = 'A';

} else if (marks >= 60) {

grade = 'B';

} else if (marks >= 50) {

grade = 'C';

} else if (marks >= 40) {

grade = 'D';

} else {

grade = 'E';

}

console.log(`The grade for ${marks} marks is ${grade}`);

}

// Example usage:

calculateGrade();